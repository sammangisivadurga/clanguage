#include <stdio.h>

int rev(int);
int main()
{
    int n,s;
    printf("\n enter value of n");
    scanf("%d",&n);
    s=rev(n);
    return 0;
}
int rev(int n)
{
    if(n==0)
    return 0;
    else
    {
        printf("\n%d",n);
        return n+rev(n-1);
    
    }
}

